			////			menu_btn.addEventListener(MouseEvent.CLICK, f_abrirMenu);			function f_abrirMenu(e: MouseEvent) {				menu_mc.visible = true;				if (Bandera_menu == 0) {					siguiente_btn.visible = false;					anterior_btn.visible = false;					Tweener.addTween(menu_mc, {						y: 9,						time: 3,						transition: "easeInQuad"					});					Bandera_menu = 1;				} else {					siguiente_btn.visible = true;					anterior_btn.visible = true;					Tweener.addTween(menu_mc, {						y: 800,						time: 3,						transition: "easeInQuad"					});					Bandera_menu = 0;				}			}			///////TODO_1: Revisar si funciona con esta versión 20140924
						function closeBrowser(e: MouseEvent): void {				ExternalInterface.call("window.top.close");			}
			////			function f_validarAnterior(d_actualSlide: Number) {				if (d_actualSlide == 1) {					mc_anterior.visible = false;					anterior_btn.visible = false;				} else {					mc_anterior.visible = true;					anterior_btn.visible = true;				}			}			function f_validarSiguiente(d_actualSlide: Number, totalSlides: Number) {				if (d_actualSlide == totalSlides) {					mc_siguiente.visible = false;					siguiente_btn.visible = false;				} else {					mc_siguiente.visible = true;					siguiente_btn.visible = true;				}			}			function f_anterior(e: MouseEvent) {				actualSlide = actualSlide - 1;				f_validarAnterior(actualSlide);				f_validarSiguiente(actualSlide, v_NumeroSubTemas);				var mi_location = String(actualSlide);				scorm.set("cmi.location", mi_location);				cargarSlides(String(actualSlide), contenedor_mc);				f_Navegados(ArregloSuspend, v_NumeroTemas, actualSlide);				pagina_txt.text = v_paginacion[actualSlide];				menu_mc.location_mc.gotoAndStop(actualSlide);				nombreSkin_txt.text = v_menuSkin[actualSlide];			}			function f_siguiente(e: MouseEvent) {				actualSlide = actualSlide + 1;				f_validarAnterior(actualSlide);				f_validarSiguiente(actualSlide, v_NumeroSubTemas);				var mi_location = String(actualSlide);				scorm.set("cmi.location", mi_location);				////Cargar				cargarSlides(String(actualSlide), contenedor_mc);				ArregloSuspend[actualSlide - 1] = 1;				f_enviarComplete(ArregloSuspend, v_NumeroSubTemas);				f_Navegados(ArregloSuspend, v_NumeroTemas, actualSlide);				/////Navegación				f_tipoNavegacion(v_TipoNavegacion, ArregloSuspend, v_NumeroSubTemas, actualSlide);				//				Enviar_ArregloSuspend = String(ArregloSuspend);				scorm.set("cmi.suspend_data", Enviar_ArregloSuspend);				pagina_txt.text = v_paginacion[actualSlide];				menu_mc.location_mc.gotoAndStop(actualSlide);				nombreSkin_txt.text = v_menuSkin[actualSlide];			}			function f_Navegados(miArregloSuspend: Array, miArregloNumeroTemas: Array, miPaginaActual: Number) {				var miNombreNavegados: String;				var miNombreNavegadosT: String;				for (var k = 1; k <= miArregloNumeroTemas.length; k++) {					if (miPaginaActual == miArregloNumeroTemas[k]) {						constante = k;						break;					}				}				if (constante >= 1) {					miNombreNavegadosT = "suspendT" + constante + "_mc";					menu_mc.getChildByName(miNombreNavegadosT).visible = true;				}				if (miArregloSuspend[miPaginaActual - 1] == 1) {					miNombreNavegados = "suspend" + miPaginaActual + "_mc";					menu_mc.getChildByName(miNombreNavegados).visible = true;				}			}			function f_llenadoNavegados(miArregloSuspend: Array, miArregloNumeroTemas: Array) {				var miLargoTemas = miArregloSuspend.length;				var sumatoriaArreglo: Number = 0;				var miNombreNavegados: String;				var miNombreNavegadosT: String;				for (var l = 0; l < miLargoTemas; l++) {					if (miArregloSuspend[l - 1] == 1) {						sumatoriaArreglo = 1 + sumatoriaArreglo;						miNombreNavegados = "suspend" + l + "_mc";						menu_mc.getChildByName(miNombreNavegados).visible = true;					}					for (var m = 1; m <= miArregloNumeroTemas.length; m++) {						if (sumatoriaArreglo == miArregloNumeroTemas[m]) {							constante = m;							break;						}					}					if (constante >= 1) {						miNombreNavegadosT = "suspendT" + constante + "_mc";						menu_mc.getChildByName(miNombreNavegadosT).visible = true;					}				}			}			function f_tipoNavegacion(_MiTipo: String, _MiArregloSuspend: Array, _NumeroSubTemas: Number, _paginaActual: Number) {				////Variables Privadas				var private_MiLargoArreglo = _MiArregloSuspend.length;				var private_MiNombreBoton: String;				///Eventos				switch (_MiTipo) {					case "lineal":						for (var n = 0; n <= private_MiLargoArreglo; n++) {							private_MiNombreBoton = "btn_" + Number(n + 1);							if (n < _NumeroSubTemas) {								if (_MiArregloSuspend[n] == 1) {									menu_mc.getChildByName(private_MiNombreBoton).visible = true;								} else {									menu_mc.getChildByName(private_MiNombreBoton).visible = false;								}							}							private_MiNombreBoton = "btn_" + _paginaActual;							menu_mc.getChildByName(private_MiNombreBoton).visible = true;						}						break;					case "libre":						for (var o = 0; o <= private_MiLargoArreglo; o++) {							private_MiNombreBoton = "btn_" + Number(o + 1);							menu_mc.getChildByName(private_MiNombreBoton).visible = true;						}						break;				}			}			function f_enviarComplete(miArregloSuspend: Array, numeroTemas: Number) {				var provi = 0;				for (var j = 0; j < miArregloSuspend.length; j++) {					provi = provi + Number(miArregloSuspend[j]);				}				var textoPuntaje = String(provi / numeroTemas);				if (provi == numeroTemas) {					scorm.set("cmi.completion_status", "completed");				} else {					scorm.set("cmi.completion_status", "incomplete");				}			}			function f_HabilitarSiguiente() {				addEventListener(Event.ENTER_FRAME, myFunction);				function myFunction(event: Event) {					//trace(mySO.data.habilitar);					trace(global.getValue("bandera"));					if (global.getValue("bandera") == true) {						mc_siguiente.visible = true;						siguiente_btn.visible = true;					} else {						mc_siguiente.visible = false;						siguiente_btn.visible = false;					}				}			}		}		////Mis Funciones		function cargarSlides(url: String, carga: UILoader) {			////			swf_archivo = "00" + url + ".swf";			var swf_descargar: URLRequest = new URLRequest(swf_archivo);			swf_Loader.load(swf_descargar);			carga.addChild(swf_Loader);		}		///		function f_arregloSuspendPrimero(totalTemas: Number): Array {			var ArregloRetorna: Array = new Array();			for (var i = 0; i < totalTemas; i++) {				ArregloRetorna[i] = 0;			}			return ArregloRetorna;		}	}}